{
  "root": "public/",
  "clean_urls": true,
  "https_only": true,
  "headers": {
    "/": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/images/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/favicon.ico": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/manifest.json": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/robots.txt": {
      "Cache-Control": "public, max-age=86400"
    },
    "/sitemap.xml": {
      "Cache-Control": "public, max-age=86400"
    }
  },
  "error_page": "404.html",
  "routes": {
    "/**": "index.html"
  }
}