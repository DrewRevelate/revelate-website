{
  "clean_urls": true,
  "https_only": true,
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=3600"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/css/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/js/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/images/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  },
  "routes": {
    "/*.html": {
      "status": 301,
      "force": true,
      "url": "/$1"
    }
  },
  "proxies": {
    "/api/": {
      "origin": "${API_URL}"
    }
  }
}
